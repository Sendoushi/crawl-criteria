{
  "name": "mrcrowley",
  "version": "0.0.1",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git@github.com:Sendoushi/mrcrowley.git"
  },
  "authors": [
    "Joel Santos <joel@sendoushi.com>"
  ],
  "bin": {
    "mrcrowley": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "clean": "rm -rf dist/*",
    "test": "npm run eslint && npm run mocha",
    "eslint": "eslint ./src/**/*.js",
    "mocha": "BABEL_ENV=test mocha ./src/**/*.spec.js ./src/**/**/*.spec.js --compilers js:babel-core/register",
    "prebuild": "npm run clean && npm run test",
    "build": "BABEL_ENV=production babel src --out-dir dist --source-maps inline --ignore *.spec.js,_test/**/*",
    "scrap-test": "node ./src/index.js --compilers js:babel-core/register --config=\"./src/_test/data/config.json\""
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.20.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-remove-code": "0.0.4",
    "babel-preset-es2015": "^6.18.0",
    "chai": "^3.5.0",
    "eslint": "^2.0.0",
    "eslint-config-defaults": "^9.0.0",
    "eslint-plugin-react": "^6.2.0",
    "mocha": "^3.2.0"
  },
  "dependencies": {
    "joi": "^9.2.0",
    "jsdom": "^9.8.3",
    "lodash": "^4.17.4",
    "tough-cookie": "^2.3.2",
    "yargs": "^6.3.0"
  },
  "keywords": []
}
