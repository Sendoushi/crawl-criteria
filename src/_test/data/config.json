{
    "projectId": "test",
    "projectName": "Test",
    "data": [
        {
            "src": "http://www.brainjar.com/java/host/test.html",
            "name": "Simple test html",
            "retrieve": {
                "content": {
                    "selector": "body p"
                }
            }
        }, {
            "src": "http://www.brainjar.com/java/host/test.html",
            "name": "Simple test html",
            "retrieve": {
                "body": {
                    "selector": "body",
                    "retrieve": {
                        "content": {
                            "selector": "p"
                        }
                    }
                }
            }
        }, {
            "src": "http://www.brainjar.com/java/host/test.html",
            "name": "Simple test html",
            "retrieve": {
                "meta": {
                    "selector": "meta",
                    "attribute": "content"
                }
            }
        }, {
            "src": "https://www.sendoushi.com/{{query}}",
            "enableJs": true,
            "waitFor": "#content",
            "throttle": 3000,
            "modifiers": {
                "query": ["posts/projects"]
            },
            "retrieve": {
                "title": {
                    "selector": ".post-item-title",
                    "ignore": ["Prokofiev"]
                }
            }
        }
    ]
